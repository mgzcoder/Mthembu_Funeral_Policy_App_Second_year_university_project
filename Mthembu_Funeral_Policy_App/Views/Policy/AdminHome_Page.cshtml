            <i class="fa fa-bars"></i>
        </li>

    </div>

    <br />
    <br />
    <center>
        @using (Html.BeginForm())
        {
            @Html.AntiForgeryToken()

        <div class="form-horizontal" align="center" style="background-color:#1D1D2C;width:40%;height:50%;margin-left:0%;font-size:20px">
            <br>
            <h3 style="color:#F6F4D5;font-size:40px">Sign In</h3>
            <hr />

            @Html.ValidationSummary(true, "", new { @class = "text-danger" })
            <div class="form-group" style="margin-left: 1%">
                @Html.LabelFor(model => model.Company_Code, htmlAttributes: new { @class = "control-label col-md-2", Style = "color:#F9F9F9" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.Company_Code, new { htmlAttributes = new { @class = "form-control", Style = "border-color: #1D1D2C" } })
                    @Html.ValidationMessageFor(model => model.Company_Code, "", new { @class = "text-danger" })
                    <label style="color: red;font-size:18px"> @ViewBag.CompanyCodeErroMessage</label>
                </div>
            </div>

            <div class="form-group" style="margin-left: 1%">
                @Html.LabelFor(model => model.Admin_UserName, htmlAttributes: new { @class = "control-label col-md-2", Style = "color:#F9F9F9" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.Admin_UserName, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Admin_UserName, "", new { @class = "text-danger" })
                </div>
            </div>

            <div class="form-group" style="margin-left: 1%">
                @Html.LabelFor(model => model.Admin_Password, htmlAttributes: new { @class = "control-label col-md-2", Style = "color:#F9F9F9" })
                <div class="col-md-10">
                    @Html.EditorFor(model => model.Admin_Password, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.Admin_Password, "", new { @class = "text-danger" })
                    <label style="color: red;font-size:18px"> @ViewBag.UsernameorPassword</label>
                </div>
            </div>
            
            <br /><br />
            <div class="form-group">
                <div class="col-md-offset-2 col-md-10">
                    <b><input type="submit" value="Sign In" style="font-size:25px; width: 100%; background-color: #4CAF50; color: #1D1D2C; padding: 14px 20px; margin-right:30%; border: none; border-radius: 4px; cursor: pointer;" /></b>
                </div>
            </div>
            <br />
            <br />
        </div>

        }
    </center>
